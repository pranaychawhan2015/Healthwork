{
  "name": "healthwork",
  "version": "1.0.0",
  "description": "HealthWork application implemented in JavaScript",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "nyc mocha --recursive",
    "build": "tsc",
    "build:watch": "tsc -w",
    "prepublishOnly": "npm run build",
    "start": "ts-node SampleQueryHandler.ts"
  },
  "engineStrict": true,
  "author": "Hyperledger",
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/plugin-transform-typescript": "^7.16.8",
    "circular-dependency-plugin": "^5.2.2",
    "couchdb": "^0.3.8",
    "ecdsa": "^0.7.0",
    "express": "^4.17.2",
    "fabric-ca-client": "^2.2.4",
    "fabric-network": "^2.2.4",
    "fabric-shim": "^2.2.2",
    "js-yaml": "^4.1.0",
    "level": "^7.0.1",
    "madge": "^5.0.1",
    "node-go-require": "^2.0.0",
    "nodemon": "^2.0.15",
    "openssl": "^2.0.0",
    "openssl-nodejs": "^1.0.5",
    "promised-io": "^0.2.3",
    "python3": "^0.0.1",
    "serve-favicon": "^2.5.0",
    "starkbank-ecdsa": "^1.1.4"
  },
  "devDependencies": {
    "@types/node": "^17.0.25",
    "chai": "^4.2.0",
    "eslint": "^8.5.0",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "sinon": "^7.1.1",
    "sinon-chai": "^3.3.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "nyc": {
    "exclude": [
      "coverage/**",
      "test/**"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "all": true,
    "check-coverage": true,
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100
  },
  "main": "app.js"
}
